{
    "main": {
        "num_cpus": 2,
        "log_level": 1,
        "channel_size": 50000
    },
    "in": {
        "module": "TCPCSVInput",
        "threads": 2,
        "listen": "0.0.0.0",
        "port": 9092,
        "headers": ["hello", "test", "src"],
        "separator": ",",
        "convert": false
    },
    "proc": [
        {
            "module": "LogProc",
            "level": "debug"
        }
    ],
    "out": {
        "module": "FileJSONOutput",
        "rotate_seconds": 60,
        "folder": "/tmp",
        "file_name_format": "gopipe-20060102-150405.json"
    }
}
