{
    "main": {
        "num_cpus": 2,
        "log_level": 1
    },
    "in": {
        "module": "TCPInput",
        "threads": 2,
        "listen": "0.0.0.0",
        "port": 9092
    },
    "proc": [
        {
            "module": "LogProc"
        }
    ],
    "out": {
        "module": "NullOutput",
        "rotate_seconds": 60,
        "file_name_format": "gopipe-%Y%M%d-%H%m%s.csv",
        "separator": ","
    }
}
